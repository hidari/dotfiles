{
  "cleanupPeriodDays": 10,
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(curl:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(tail:*)",
      "Bash(tree:*)",
      "Bash(git add:*)",
      "Bash(git log:*)",
      "Bash(cargo test:*)",
      "Bash(cargo build:*)",
      "Bash(cargo clippy:*)",
      "Bash(cargo fmt:*)",
      "Bash(gh run list:*)",
      "Bash(gh run view:*)",
      "Bash(pnpm vitest run:*)",
      "Bash(pnpm test:*)",
      "Skill(code-reviewer)",
      "Skill(frontend-designer)",
      "WebSearch"
    ],
    "deny": [
      "NoteboolEdit",
      "NotebookRead"
    ],
    "ask": [
      "Bash(git commit:*)"
    ]
  },
  "outputStyle": "Explanatory",
  "alwaysThinkingEnabled": true
}
